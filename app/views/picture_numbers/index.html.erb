<p id="notice"><%= notice %></p>

<h1>Listing Picture Numbers</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @picture_numbers.each do |picture_number| %>
      <tr>
        <td><%= picture_number.title %></td>
        <td><%= link_to 'Show', picture_number %></td>
        <td><%= link_to 'Edit', edit_picture_number_path(picture_number) %></td>
        <td><%= link_to 'Destroy', picture_number, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Picture number', new_picture_number_path %>
